{
  "name": "@altalune/svelte-adapter-serverless",
  "version": "0.1.0",
  "description": "Adapter for SvelteKit apps that generates a serverless handler",
  "repository": {
    "type": "git",
    "url": "https://github.com/hrz8/svelte-adapter-serverless"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.js"
    },
    "./package.json": "./package.json"
  },
  "types": "index.d.ts",
  "files": [
    "files",
    "index.js",
    "index.d.ts"
  ],
  "scripts": {
    "dev": "node -e \"fs.rmSync('files', { force: true, recursive: true })\" && rollup -cw",
    "build": "node -e \"fs.rmSync('files', { force: true, recursive: true })\" && rollup -c"
  },
  "devDependencies": {
    "@polka/url": "1.0.0-next.24",
    "@sveltejs/kit": "^2.5.0",
    "@types/node": "^18.19.3",
    "polka": "1.0.0-next.24",
    "sirv": "^2.0.4",
    "typescript": "^5.3.3",
		"svelte": "^4.2.9",
		"vite": "^5.0.12"
  },
  "dependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "rollup": "^4.9.5"
  },
  "peerDependencies": {
    "@sveltejs/kit": "^2.5.0",
		"svelte": "^4.2.9",
		"vite": "^5.0.12"
  }
}
